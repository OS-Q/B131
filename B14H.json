{
  "build": {
    "core": "sduino",
    "extra_flags": "-DB14H -DSTM8S208",
    "f_cpu": "16000000L",
    "cpu": "stm8",
    "mcu": "stm8s208mbt6",
    "variant": "B14H"
  },
  "debug": {
    "openocd_target": "stm8s",
    "tools": {
      "stlink": {
        "default": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/interface/stlink.cfg",
            "-f",
            "scripts/target/stm8s.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        },
        "onboard": true
      }
    }
  },
  "frameworks": [
    "arduino",
    "native"
  ],
  "upload": {
    "maximum_ram_size": 6144,
    "maximum_size": 131072,
    "protocol": "serial",
    "protocols": [
      "stlinkv2",
      "serial"
    ]
  },
  "name": "STM8S208MB",
  "url": "https://www.st.com/zh/microcontrollers-microprocessors/stm8s207-208.html",
  "vendor": "OS-Q"
}
