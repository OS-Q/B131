{
  "build": {
    "core": "sduino",
    "extra_flags": "-DB14A -DSTM8S001",
    "f_cpu": "16000000L",
    "cpu": "stm8",
    "mcu": "stm8s001j3p6",
    "variant": "B14A"
  },
  "debug": {
    "openocd_target": "stm8s",
    "tools": {
      "stlink": {
        "default": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/interface/stlink.cfg",
            "-f",
            "scripts/target/stm8s.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        },
        "onboard": true
      }
    }
  },
  "frameworks": [
    "arduino",
    "spl",
    "ino",
    "native"
  ],
  "upload": {
    "maximum_ram_size": 1024,
    "maximum_size": 8192,
    "protocol": "stlinkv2",
    "protocols": [
        "serial",
        "stlinkv2"
    ]
  },
  "name": "STM8S001J3",
  "url": "https://www.st.com/zh/microcontrollers-microprocessors/stm8s001j3.html",
  "vendor": "OS-Q"
}
